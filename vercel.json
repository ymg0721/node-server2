{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/reservation.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/ec.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/send-email",
      "headers": {
        "Access-Control-Allow-Origin": "https://salone-new-flower.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      },
      "dest": "src/server.js"
    },
    {
      "src": "/send-reservation",
      "headers": {
        "Access-Control-Allow-Origin": "https://salone-new-flower.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      },
      "dest": "src/reservation.js"
    },
    {
      "src": "/create-checkout-session",
      "headers": {
        "Access-Control-Allow-Origin": "https://salone-new-flower.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      },
      "dest": "src/ec.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/ec.js",
      "methods": ["GET", "POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://salone-new-flower.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, sec-ch-ua, sec-ch-ua-mobile, sec-ch-ua-platform, User-Agent, Referer, stripe-signature",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
